{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/log.txt",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 7,
                "buffered": true
              }
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*",

  "DatabaseSettings": {
    "CollectionName": "AppData",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "GSUMuseum"
  },

  "X-API-KEY": "U3VwZXJTZWNyZXRBcGlLZXkxMjM"
}
